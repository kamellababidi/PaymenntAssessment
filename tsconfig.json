{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "experimentalDecorators": true,
      "baseUrl": "./src",
      "paths": {
        "@assets/*": ["assets/*"],
        "@components/*": ["components/*"],
        "@enums/*": ["enums/*"],
        "@models/*": ["models/*"],
        "@screens/*": ["screens/*"],
        "@navigator/*": ["navigator/*"],
        "@constants/*": ["constants/*"],
        "@utils": ["utils"],
        "@hooks": ["hooks"]
      }
    }
  }
  