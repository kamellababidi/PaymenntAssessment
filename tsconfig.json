{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "experimentalDecorators": true,
      "baseUrl": "./src",
      "paths": {
        "@assets/*": ["assets/*"],
        "@images/*": ["assets/images/*"],
        "@components/*": ["components/*"],
        "@enums/*": ["enums/*"],
        "@models/*": ["models/*"],
        "@sagas": ["sagas"],
        "@screens/*": ["screens/*"],
        "@navigator/*": ["navigator/*"],
        "@constants/*": ["constants/*"],
        "@store": ["store"],
        "@uikit": ["uikit"],
        "@utils": ["utils"]
      }
    }
  }
  